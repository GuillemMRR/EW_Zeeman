Cell[CellGroupData[{

Cell["1 Local operators and their collective representation", "Chapter",
 CellChangeTimes->{{3.8574555226484613`*^9, 3.857455537296421*^9}, {
  3.8574556013481445`*^9, 
  3.857455605426898*^9}},ExpressionUUID->"414941f2-9be8-4c2e-8d1a-\
270df349c989"],

Cell["\<\
First we define the Spin-1 and SU(3) generators matrices (Gell-Mann) in the \
fundamental representation. Then, we compute the corresponding matrix \
representations of the SU(3) generators on the totally symmetric subspace of \
the Np atoms, MatSU3, with the SUN  package.  \
\>", "Text",
 CellChangeTimes->{
  3.8574610440332584`*^9},ExpressionUUID->"3021a567-e6e5-4440-a42f-\
4faa44048c3b"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"MatSpin", "=", 
    RowBox[{
     RowBox[{"LieAlgebraBasisMatrices", "[", 
      RowBox[{"Irrep", "[", "2", "]"}], "]"}], "[", 
     RowBox[{"[", "1", "]"}], "]"}]}], ";"}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{"Spin", "-", 
    RowBox[{"1", " ", "matrices"}]}], " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"MatGM", "=", 
    RowBox[{"N", "[", 
     RowBox[{
      RowBox[{"LieAlgebraBasisMatrices", "[", 
       RowBox[{"Irrep", "[", 
        RowBox[{"1", ",", "0"}], "]"}], "]"}], "[", 
      RowBox[{"[", "1", "]"}], "]"}], "]"}]}], ";"}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{"Gell", "-", 
    RowBox[{"Mann", " ", "SU", 
     RowBox[{"(", "3", ")"}], " ", "matrices"}]}], " ", 
   "*)"}]}], "\[IndentingNewLine]"}], "Input",
 CellChangeTimes->{{3.8574451077830973`*^9, 3.857445166846339*^9}, 
   3.8574607041823688`*^9},
 CellLabel->"In[4]:=",ExpressionUUID->"936b14d8-b67d-4a65-9c99-d598a817086f"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"MatSU3", "=", 
    RowBox[{"N", "[", 
     RowBox[{
      RowBox[{"LieAlgebraBasisMatrices", "[", 
       RowBox[{"Irrep", "[", 
        RowBox[{"Np", ",", "0"}], "]"}], "]"}], "[", 
      RowBox[{"[", "1", "]"}], "]"}], "]"}]}], " ", ";", " ", 
   RowBox[{"(*", 
    RowBox[{"Collective", " ", "representation", " ", "of", " ", "SU", 
     RowBox[{"(", "3", ")"}], " ", "matrices"}], "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{"dim", "=", 
    RowBox[{
     RowBox[{"Dimensions", "[", "MatSU3", "]"}], "[", 
     RowBox[{"[", "2", "]"}], "]"}]}], ";"}], " ", 
  RowBox[{"(*", 
   RowBox[{"And", " ", "its", " ", "dimension"}], "*)"}], 
  "\[IndentingNewLine]"}]], "Input",
 CellChangeTimes->{{3.857445172679617*^9, 3.8574452065794454`*^9}, {
   3.857445905406129*^9, 3.8574459082367134`*^9}, 3.857460701467182*^9},
 CellLabel->"In[6]:=",ExpressionUUID->"6594d550-42c4-474a-a389-fcc26746073b"],

Cell[TextData[{
 "For a arbitrary local operator ",
 StyleBox["o",
  FontSlant->"Italic"],
 ", we compute the collective matrix representation ",
 StyleBox["Ir[o] := O",
  FontSlant->"Italic"]
}], "Text",
 CellChangeTimes->{{3.8574458090613637`*^9, 3.8574458517380056`*^9}, {
  3.857446266046175*^9, 3.8574462663131323`*^9}, {3.8574465819224944`*^9, 
  3.8574465932511654`*^9}},ExpressionUUID->"b66d97db-f879-48ca-84df-\
ed325ef85ee8"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"Ir", "[", "o_", "]"}], ":=", 
   RowBox[{
    RowBox[{"Sum", "[", 
     RowBox[{
      RowBox[{"2", "*", 
       RowBox[{"Tr", "[", 
        RowBox[{"o", ".", 
         RowBox[{"MatGM", "[", 
          RowBox[{"[", "a", "]"}], "]"}]}], "]"}], "*", 
       RowBox[{"MatSU3", "[", 
        RowBox[{"[", "a", "]"}], "]"}]}], ",", 
      RowBox[{"{", 
       RowBox[{"a", ",", "1", ",", 
        RowBox[{"First", "[", 
         RowBox[{"Dimensions", "[", "MatSU3", "]"}], "]"}]}], "}"}]}], "]"}], 
    "+", 
    RowBox[{"Np", "*", 
     RowBox[{"Tr", "[", "o", "]"}], "*", 
     RowBox[{
      RowBox[{"IdentityMatrix", "[", 
       RowBox[{"dim", ",", " ", "SparseArray"}], "]"}], "/", 
      RowBox[{"First", "[", 
       RowBox[{"Dimensions", "[", 
        RowBox[{"MatGM", "[", 
         RowBox[{"[", "1", "]"}], "]"}], "]"}], "]"}]}]}]}]}], ";"}]], "Input",
 CellChangeTimes->{
  3.8574456911337028`*^9, 3.857445747628179*^9, {3.857445778180584*^9, 
   3.85744580541223*^9}, {3.857446270456708*^9, 3.8574462782331867`*^9}, {
   3.8574623651570935`*^9, 3.857462365575897*^9}, {3.85746244539432*^9, 
   3.857462445814211*^9}, {3.8574624821525927`*^9, 3.8574624832608905`*^9}},
 CellLabel->"In[7]:=",ExpressionUUID->"07dc7628-b775-4f8e-baa5-75ba9ba1c19e"],

Cell[TextData[{
 "The projector onto outcome ",
 StyleBox["m",
  FontSlant->"Italic"],
 " of a local observable ",
 StyleBox["o",
  FontSlant->"Italic"],
 ".  Will be used in the following to compute the magnetic-sublevel single \
atom projectors from the Spin-1 observables. "
}], "Text",
 CellChangeTimes->{{3.857445862575145*^9, 3.857445865238338*^9}, {
  3.8574459416247716`*^9, 3.857446061058197*^9}, {3.857453659550802*^9, 
  3.8574536598105617`*^9}, {3.8574610778864284`*^9, 
  3.857461078216213*^9}},ExpressionUUID->"2ec4c59a-1465-4918-bcbd-\
187f29ffe7f2"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Pro", "[", 
    RowBox[{"o_", ",", "m_"}], "]"}], ":=", 
   RowBox[{"Sum", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"KroneckerDelta", "[", 
       RowBox[{"m", ",", 
        RowBox[{
         RowBox[{
          RowBox[{"Eigensystem", "[", "o", "]"}], "[", 
          RowBox[{"[", "1", "]"}], "]"}], "[", 
         RowBox[{"[", "a", "]"}], "]"}]}], "]"}], "*", 
      RowBox[{"KroneckerProduct", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"Eigensystem", "[", "o", "]"}], "[", 
           RowBox[{"[", "2", "]"}], "]"}], "[", 
          RowBox[{"[", "a", "]"}], "]"}], "\[Conjugate]"}], ",", 
        RowBox[{
         RowBox[{
          RowBox[{"Eigensystem", "[", "o", "]"}], "[", 
          RowBox[{"[", "2", "]"}], "]"}], "[", 
         RowBox[{"[", "a", "]"}], "]"}]}], "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"a", ",", "1", ",", 
       RowBox[{"First", "[", 
        RowBox[{"Dimensions", "[", "o", "]"}], "]"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Proj", "[", 
    RowBox[{"o_", ",", "m_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"Pro", "[", 
     RowBox[{"o", ",", "m"}], "]"}], "/", 
    RowBox[{"Tr", "[", 
     RowBox[{"Pro", "[", 
      RowBox[{"o", ",", "m"}], "]"}], "]"}]}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.8574462862971478`*^9, 3.8574463168197527`*^9}, {
  3.8574675228940635`*^9, 3.85746752599157*^9}},
 CellLabel->"In[8]:=",ExpressionUUID->"b6b4ac41-48cd-472b-885e-1bc4afcbb63c"],

