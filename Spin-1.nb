Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"betapsi", "[", 
    RowBox[{"o_", ",", "psi_"}], "]"}], ":=", 
   RowBox[{"Total", "[", 
    RowBox[{
     RowBox[{"Table", "[", " ", 
      RowBox[{
       RowBox[{"First", "[", 
        RowBox[{"First", "[", 
         RowBox[{
          RowBox[{"Abs", "[", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"psi", ".", 
              RowBox[{"o", "[", 
               RowBox[{"[", 
                RowBox[{"a", ",", "m"}], "]"}], "]"}], ".", 
              RowBox[{"psi", "\[ConjugateTranspose]"}]}], ")"}], "/", 
            RowBox[{"(", 
             RowBox[{"psi", ".", 
              RowBox[{"psi", "\[ConjugateTranspose]"}]}], ")"}]}], "]"}], "^",
           "2"}], "]"}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"a", ",", "1", ",", "3"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"m", ",", "1", ",", "2"}], "}"}]}], "]"}], ",", "2"}], 
    "]"}]}], ";", 
  RowBox[{"betaTmp", "=", 
   RowBox[{"Table", "[", 
