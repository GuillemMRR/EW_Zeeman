          RowBox[{"CircularUnitaryMatrixDistribution", "[", "3", "]"}], "]"}],
          "[", 
         RowBox[{"[", "1", "]"}], "]"}], "}"}]}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", "500000"}], "}"}]}], "]"}]}], 
